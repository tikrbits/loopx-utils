    "test:ci": "npm run test -- --maxWorkers=4 && cat ./coverage/lcov.info | coveralls"
  },
  "description": "Fast, small and purely functional utility library",
  "repository": "https://github.com/loopx/utils",
  "bugs": "https://github.com/loopx/utils/issues",
  "keywords": [
    "loopx",
    "utils",
    "ramda",
    "lodash",
    "underscore",
    "curry",
    "performance"
