    "inquirer": "^7.3.3",
    "jest": "^26.6.1",
    "jsdom": "^16.4.0",
    "ora": "^5.1.0",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-is": "^16.13.1",
    "recursive-readdir-sync": "^1.0.6",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5",
    "walker": "^1.0.7"
