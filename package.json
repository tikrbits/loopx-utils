  "sideEffects": false,
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@types/fs-extra": "^9.0.2",
    "@types/jest": "^26.0.15",
    "babel-core": "^6.26.3",
    "benchmark": "^2.1.4",
    "chalk": "^4.1.0",
    "coveralls": "^3.1.0",
    "esdoc": "^1.1.0",
